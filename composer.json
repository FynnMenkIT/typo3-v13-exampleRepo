{
	"name": "fynnmenkit/typo3-v13-examplerepo",
	"description": "Modern TYPO3 v13+ demo distribution with Content Blocks, MASK, and automated demo content import. Clean, accessible, and best-practice setup for agencies and freelancers.",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"homepage": "https://github.com/FynnMenkIT/typo3-v13-exampleRepo",
	"repository": "https://github.com/FynnMenkIT/typo3-v13-exampleRepo.git",
	"authors": [
		{"name": "Fynn Menk", "email": "your@email.com"}
	],
	"keywords": ["typo3", "demo", "content blocks", "mask", "distribution", "proof of concept"],
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"platform": {
			"php": "8.2"
		},
		"sort-packages": true
	},
	"repositories": {
		"local-packages": {
			"type": "path",
			"url": "packages/*"
		}
	},
	"require": {
		"demo/mask_elements": "@dev",
		"friendsoftypo3/content-blocks": "^1.3",
		"friendsoftypo3/headless": "^4.6",
		"mask/mask": "^9.0",
		"praetorius/vite-asset-collector": "^1.11",
		"typo3/cms-backend": "^13.0",
		"typo3/cms-belog": "^13.0",
		"typo3/cms-beuser": "^13.0",
		"typo3/cms-core": "^13.0",
		"typo3/cms-dashboard": "^13.0",
		"typo3/cms-extbase": "^13.0",
		"typo3/cms-extensionmanager": "^13.0",
		"typo3/cms-felogin": "^13.0",
		"typo3/cms-filelist": "^13.0",
		"typo3/cms-fluid": "^13.0",
		"typo3/cms-fluid-styled-content": "^13.0",
		"typo3/cms-form": "^13.0",
		"typo3/cms-frontend": "^13.0",
		"typo3/cms-impexp": "^13.0",
		"typo3/cms-info": "^13.0",
		"typo3/cms-install": "^13.0",
		"typo3/cms-rte-ckeditor": "^13.0",
		"typo3/cms-scheduler": "^13.0",
		"typo3/cms-seo": "^13.0",
		"typo3/cms-setup": "^13.0",
		"typo3/cms-sys-note": "^13.0",
		"typo3/cms-tstemplate": "^13.0",
		"typo3/cms-viewpage": "^13.0",
		"typo3/site-distribution": "@dev"
	},
	"require-dev": {
		"deployer/deployer": "^7.3",
		"phpstan/phpstan": "^1.10",
		"typo3/cms-lowlevel": "^13.0",
		"typo3/coding-standards": "^0.8.x-dev"
	}
}
